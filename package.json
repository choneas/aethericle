{
  "name": "aethericle",
  "version": "0.1.0",
  "packageManager": "bun@1.2.4",
  "private": true,
  "scripts": {
    "dev:next": "NODE_ENV=development next dev --turbopack",
    "dev:electron": "NODE_ENV=development tsc -p ./electron && electron ./electron/dist/main.js",
    "dev": "concurrently \"bun run dev:next\" \"bun run dev:electron\"",
    "build:next": "cross-env NODE_ENV=production next build",
    "build:electron": "cross-env NODE_ENV=production tsc -p ./electron && electron-builder",
    "build": "bun run build:next && bun run build:electron",
    "start": "cross-env NODE_ENV=production next start"
  },
  "dependencies": {
    "@cuckoo-verse/auth": "workspace:*",
    "@heroui/react": "^2.7.6-beta.2",
